[cases]
folder = <path>
  * Path to the cases folder where to find the cases.json file to be processed by the scripted input.
[logging]
level = <integer>
  * Logging level. Possible values: DEBUG (10), INFO (20), WARNING (30), ERROR (40), CRITICAL (50)
max_size_mb = <integer>
  * Maximum size of the log file in MB before rotation.
max_backup_files = <integer>
  * Number of backup files to keep.